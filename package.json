{
  "name": "inventario-ropa-tipica-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" --names \"Backend,Frontend\" --prefix-colors \"blue,green\"",
    "backend": "dotnet run",
    "frontend": "cd ClientApp && npm run dev",
    "build": "cd ClientApp && npm run build && dotnet build",
    "start": "dotnet run",
    "publish": "cd ClientApp && npm run build && dotnet publish -c Release"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^9.2.1",
    "vite": "^7.1.7"
  }
}
